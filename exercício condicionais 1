#include <stdio.h>

int main() {
    int idade;
    
    printf("Digite sua idade: ");
    scanf("%d", &idade);
    
    if(idade < 16) {
        printf("NÃ£o pode votar.\n");
    } else if(idade >= 16 && idade < 18) {
        printf("Voto facultativo.\n");
    } else {
        printf("Voto obrigatÃ³rio.\n");
    }
    return 0;
}
